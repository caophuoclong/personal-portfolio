# Example environment configuration
# Copy this file to .env and fill in your actual values

# Telegram Bot Configuration (Required for contact form)
TELEGRAM_BOT_MESSAGE_TOKEN=your_telegram_bot_token_here
TELEGRAM_CHAT_ID=your_telegram_chat_id_here

# Webhook Security (Optional but recommended)
WEBHOOK_AUTH_TOKEN=your_secure_webhook_token_here

# Admin API Access (Required for admin endpoints)
ADMIN_API_KEY=your_secure_admin_api_key_here

# Optional: Development Configuration
NODE_ENV=development
DEBUG=true

# Note: In production (Deno Deploy), set these environment variables
# through the Deno Deploy dashboard, not through this file.